<link href="https://fonts.googleapis.com/css?family=Cinzel+Decorative" rel="stylesheet">

<!--Start Rocket Thruster Sound Loop-->
<audio id="myAudio" autoplay loop>
  <source src="/assets/Rocket Thrusters.mp3" type="audio/mpeg">
</audio>
<!--End Rocket Thruster Sound Loop-->

<!--start header-->
  <div id="header_div">
    <nav>
      <h1 id="name_h1">IST</h1>
      <img id="a_pic" src="/assets/letterA.png" style ="width:70px;">
      <p id="home_p"><a href ="/">HOME</a></p>
      <p id="careers_p"><a href ="/careers">CAREERS</a></p>
      <p id="service_p"><a href ="/services">SERVICES</a></p>
      <p id="gallery_p"><a href ="/gallery">GALLERY</a></p>
    </nav>
  </div>
<!--end header-->

<!--start main-->
<div id="main_div2">
  <img id="main_pic" src="/assets/in_upPic.gif" style ="width:1325px; height:600px;">
  <div id="form_div">
    <h2 id="signup_h2">Sign up</h2>
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    <%= devise_error_messages! %>
    <div id="email_div">
      <%= f.label "Email:" %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
    </div>
    <div id="password_div">
      <%= f.label "Password:" %>
      <% if @minimum_password_length %>
      <% end %><br />
      <%= f.password_field :password, autocomplete: "off" %>
    </div>
    <div id="confirm_div">
      Confirm:<br />
      <%= f.password_field :password_confirmation, autocomplete: "off" %>
    </div>
    <div id="signup_button2">
      <%= f.submit "Sign up" %>
    </div>
    <% end %>
    <p id="login_p2">Already a member <%= render "devise/shared/links" %></p>
  </div>
</div>
<!--end main-->

<!--Start footer-->
